  <%= render(:erb, :_header) %>
  <%= render(:erb, :_nav_bar) %>
  <section class="main">
    <section class="box" id="posts">
    <h2 class="section_title"> Most Recent Posts </h2>
    <% @docs.each_with_index do |doc, index| %>
          <%= render(:erb, :_doc_preview, :locals => {:doc => doc}) %>
    <% end %>
    <% if @docs.length == 10 %><a href="/documents?first=<%= @first + 10 %>">NEXT</a><%end%>
        <% if @docs.length < 10 %><a href="/documents?first=<%= @first -10 %>">PREVIOUS</a><%end%>
     </section>
  </section>
